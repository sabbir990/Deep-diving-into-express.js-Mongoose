{
  "compilerOptions": {
   "target": "ES2019",
    "module": "Node16",
    "moduleResolution": "Node16",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true                       /* Skip type checking all .d.ts files. */
  }
}
